{
  "expo": {
    "name": "mood-check-in",
    "slug": "mood-check-in",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.2",
    "orientation": "default",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "description": "App for recording your moods by doing a \"check-in\" from time to time.",
    "githubUrl": "https://github.com/louiechristie/mood-check-in"
  }
}
